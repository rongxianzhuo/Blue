#pragma kernel CSMain

uint src_height;
uint src_width;
StructuredBuffer<float> from;
RWStructuredBuffer<float> to;

[numthreads(64, 1, 1)]
void CSMain(uint3 id : SV_DispatchThreadID)
{
    const uint i = id.x % src_height;
    const uint j = id.x / src_height;
    to[id.x] = from[i * src_width + j];
}